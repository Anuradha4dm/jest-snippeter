{
  "name": "jest-snippeter",
  "displayName": "jest-snippeter",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "jest-snippeter.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "jest-snippeter.createTestFile",
        "title": "Create Unit Test File"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "resourceLangId == javascript || resourceLangId == typescript",
          "command": "jest-snippeter.createTestFile",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "jest",
    "lint": "eslint src --ext ts",
    "test:watch": "jest --watch",
    "fresh-install": "rm -rf package-lock.json node_modules"
  },
  "devDependencies": {
    "@babel/core": "7.24.8",
    "@babel/parser": "^7.24.8",
    "@babel/preset-env": "7.24.8",
    "@babel/preset-typescript": "7.24.7",
    "@babel/types": "7.24.8",
    "@sinclair/typebox": "^0.32.34",
    "@types/jest": "29.5.12",
    "@types/node": "20.x",
    "@types/vscode": "1.91.0",
    "@typescript-eslint/eslint-plugin": "7.14.1",
    "@typescript-eslint/parser": "7.11.0",
    "@vscode/test-cli": "0.0.9",
    "@vscode/test-electron": "2.4.0",
    "babel-jest": "29.7.0",
    "eslint": "8.57.0",
    "jest": "29.7.0",
    "typescript": "5.4.5"
  },
  "dependencies": {
    "vscode": "1.1.37"
  }
}
